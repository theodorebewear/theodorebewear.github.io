    const generationImages = {
      "GEN 1": "Gen1.jpg",
      "GEN 2 - NEO": "Gen2.jpg",
      "GEN 3 - EX": "Gen3.jpg",
      "GEN 4 - DPPt / HGSS": "Gen-4.jpg",
      "GEN 5 - Black White": "Gen5.jpg",
      "GEN 6 - XY": "Gen6.jpg",
      "GEN 7 - Sun Moon": "Gen7.jpg",
      "GEN 8 - Sword Shield": "Gen8.jpg",
      "GEN 9 - Scarlet & Violet": "Gen9.jpg"

    };

    const generationSelect = document.getElementById('generationSelect');
    const setSelect = document.getElementById('setSelect');
    const raritySelect = document.getElementById('raritySelect');
    const resultDiv = document.getElementById('result');
    const breakdownDiv = document.getElementById('breakdown');
    const backgroundBox = document.getElementById('backgroundBox');

    function populateGenerations() {
      for (let generation in data) {
        const option = document.createElement('option');
        option.value = generation;
        option.textContent = generation;
        generationSelect.appendChild(option);
      }

      updateSets();
      updateBackground();
    }

    function updateSets() {
      const generation = generationSelect.value;
      const sets = data[generation];

      setSelect.innerHTML = '';

      for (let setName in sets) {
        const option = document.createElement('option');
        option.value = setName;
        option.textContent = setName;
        setSelect.appendChild(option);
      }

      updateRarities();
      updateBackground();
    }

    function updateRarities() {
      const generation = generationSelect.value;
      const setName = setSelect.value;
      const rarities = data[generation][setName].rarities;

      raritySelect.innerHTML = '';

      for (let rarity in rarities) {
        const option = document.createElement('option');
        option.value = rarity;
        option.textContent = rarity;
        raritySelect.appendChild(option);
      }

      calculateResult();
    }

    function updateBackground() {
      const generation = generationSelect.value;
      const imageUrl = generationImages[generation];

      if (imageUrl) {
        backgroundBox.style.backgroundImage = `url('${imageUrl}')`;
      } else {
        backgroundBox.style.backgroundImage = 'none';
      }
    }

    function calculateResult() {
      const generation = generationSelect.value;
      const setName = setSelect.value;
      const rarityName = raritySelect.value;

      if (!generation || !setName || !rarityName) return;

      const setData = data[generation][setName];
      const meta = setData.meta;
      const rarity = setData.rarities[rarityName];

      const { average_produced, in_pack } = meta;
      const { pull_rate, number_in_set } = rarity;

      let result = average_produced / in_pack / pull_rate / number_in_set;

      const formattedResult = result.toLocaleString(undefined, { minimumFractionDigits: 2, maximumFractionDigits: 2 });
      const formattedAvg = average_produced.toLocaleString();
      const formattedInPack = in_pack.toLocaleString();
      const formattedPullRate = pull_rate.toLocaleString(undefined, { minimumFractionDigits: 2 });
      const formattedNumberInSet = number_in_set.toLocaleString();

      resultDiv.innerHTML = `Result: <strong>${formattedResult}</strong>`;
      breakdownDiv.innerHTML = `(${formattedAvg} / ${formattedInPack} / ${formattedPullRate} / ${formattedNumberInSet})`;
    }

    generationSelect.addEventListener('change', () => {
      updateSets();
      updateBackground();
    });

    setSelect.addEventListener('change', () => {
      updateRarities();
    });

    raritySelect.addEventListener('change', calculateResult);

    populateGenerations();
  </script>

<p>( Average Units Produced per Set / Cards in Pack / Pull Rate per Pack / Rarity Tier in Set ) </p>

<p>// Please note: <I>This calculator does not take into account Language Percentage, Promos & Theme Decks, or differences between Japanese and Foreign Language set makeup / pull rate. Average Units Produced is major assumption. This should be used as a fun thought exercise and not a definitive answer to card print runs. Though, without any of the aforementioned variables we perhaps get a padded "ceiling" on the number of likely produced cards. </i> </p>

</body>
</html>
